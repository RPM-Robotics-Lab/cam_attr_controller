package irp_sen;

/**
 * IRAP colored local point cloud
 * @brief IRAP colored local point cloud
 */

struct cpc_t 
{
    int64_t utime;              //!< sensor timestamp in utime [usec]

    int32_t n;                  //!< number of points

    float x[n];                 //!< x coordinate of points [m]
    float y[n];                 //!< y coordinate of points [m]
    float z[n];                 //!< z coordinate of points [m]

    color_float_t colors[n];    //!< color of points
}
