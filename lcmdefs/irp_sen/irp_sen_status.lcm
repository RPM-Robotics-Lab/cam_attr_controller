package irp_sen;

/**
 * IRAP sensor data: vehicle status
 * @brief vehicle status
 */

struct status_t 
{

    int64_t utime;          //!< sensor timestamp in utime [usec]

    float voltage;          //!< battary voltage

    boolean velodyne[2];    //!< velodyne status (1: on, 0: off)

    boolean camera[4];      //!< camera status (1: on, 0: off)
}

